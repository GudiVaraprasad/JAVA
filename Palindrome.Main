import java.util.*;
class Main
{
    public static void main(String args[])
    {
    int A,B,C,sum=0;
    Scanner in=new Scanner(System.in);
    A=in.nextInt();
    C=A;
    while(A>0)
    {
       B=A%10;
       A=A/10;
       sum=sum*10+B;
    }
    if (sum==C)
    System.out.println("It is a Palindrome");
    else
    System.out.println("It is not a Palindrome");
    
    }    
}